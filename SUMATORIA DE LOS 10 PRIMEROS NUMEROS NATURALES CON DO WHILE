    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {  
        int suma = 0;  
        
        int i = 0;
        
        do {
            suma = suma + i;
            i++;
        }
        
        while (i <= 10);
        
        JOptionPane.showMessageDialog(null,"La suma de los 10 primeros numeros naturales es : " + suma);
        
    }
    
}
